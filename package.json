{
  "name": "replaydash",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:sdk": "npm run dev --workspace=@replaydash/sdk",
    "dev:api": "npm run dev --workspace=@replaydash/api",
    "dev:dashboard": "npm run dev --workspace=@replaydash/dashboard",
    "build:sdk": "npm run build --workspace=@replaydash/sdk",
    "build:api": "npm run build --workspace=@replaydash/api",
    "build:dashboard": "npm run build --workspace=@replaydash/dashboard",
    "build": "npm run build:sdk && npm run build:api && npm run build:dashboard"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
